{
  "name": "keysha-auth-service",
  "version": "1.0.0",
  "description": "Authentication service for Keysha - Google OAuth 2.0 with PKCE",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "npm run build && ./deploy.sh deploy",
    "deploy:dev": "npm run build && ./deploy.sh deploy --stage dev",
    "deploy:prod": "npm run build && ./deploy.sh deploy --stage prod",
    "remove": "serverless remove",
    "logs": "serverless logs -f api -t"
  },
  "keywords": [
    "oauth",
    "authentication",
    "jwt",
    "mongodb"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "googleapis": "^128.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "serverless-http": "^3.2.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "dotenv-cli": "^11.0.0",
    "eslint": "^8.56.0",
    "serverless": "^3.38.0",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-offline": "^13.3.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
